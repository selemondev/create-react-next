{
  "name": "create-react-next",
  "version": "1.0.1",
  "description": "The Next Generation React Scaffolding Tool âœ¨",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "esno src/index.ts",
    "build:package": "tsup-node src/index.ts --format cjs,esm --clean --dts --minify --shims",
    "generate:release": "npx changelogen@latest --release",
    "package:beta": "npm run build && npm publish --tag beta",
    "package": "pnpm build:package && npm publish --access=public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "create-react-next": "./dist/index.js"
  },
  "files": [
    "dist",
    "src",
    "template"
  ],
  "keywords": [
    "create-react-next",
    "React 18 CLI",
    "React CLI",
    "Vite CLI",
    "Vite",
    "React-ts",
    "React-js",
    "TypeScript"
  ],
  "author": "Selemondev",
  "license": "MIT",
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.30",
    "@types/prompts": "^2.4.9",
    "@types/validate-npm-package-name": "^4.0.2",
    "conf": "^12.0.0",
    "esno": "^4.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "ejs": "^3.1.9",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.1",
    "prettier": "^3.5.3",
    "prompts": "^2.4.2",
    "validate-npm-package-name": "^5.0.0"
  },
  "pnpm": {
    "overrides": {},
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}